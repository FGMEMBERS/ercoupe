<?xml version="1.0"?>

<PropertyList>
 
 <path>Magnetos.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Key</object-name>
  <property>controls/engines/engine/magnetos</property>
  <factor>-15</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>knob</type>
  <object-name>Key</object-name>
  <value>0.0</value>
  <action>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine/magnetos</property>
    <factor>1</factor>
   </binding>
  </action>
  <hovered>
   <binding>
    <command>set-tooltip</command>
    <label>Magnetos: %s</label>
    <measure-text>Magnetos: Off</measure-text>
    <tooltip-id>magnetos</tooltip-id>
    <property>controls/engines/engine/magnetos</property>
    <mapping>nasal</mapping>
    <script>
     var modes = ['Off', '1st', '2nd', 'Both'];
     return modes[arg[0]];
    </script>
   </binding>
  </hovered>
 </animation>

</PropertyList>
